@startuml modelagem_dominio
class Cliente {
+nome: String
+email: String
+endereco: String
+telefone: String
}
class Produto {
+nome: String

+descricao: String
+preco: Double
+quantidadeEstoque: int
}
class Pedido {
+data: Date
+valorTotal: Double
+status: String
}
class ItemPedido {
+quantidade: int
